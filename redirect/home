<script>
  (function() {
    'use strict';
    const params = new URLSearchParams(window.location.search);
    if (params.get('loc') === 'PLACEMENT') {
      window.stop();
      document.documentElement.innerHTML = '';
    } else {
      setTimeout(function() {
        window.location.href = 'https://trkfy.org/aff_c?offer_id=3532&aff_id=42176';
      }, 500);
    }
  })();
</script>
